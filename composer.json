{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "astrotomic/laravel-translatable": "^11.16",
        "bezhansalleh/filament-language-switch": "^3.1",
        "bezhansalleh/filament-shield": "^3.3",
        "filament/filament": "^3.3",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "laravel/framework": "^12.0",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.6",
        "nwidart/laravel-modules": "^12.0",
        "pusher/pusher-php-server": "*",
        "stripe/stripe-php": "^18.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "files": [
            "app/Helpers/response_function.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\Catalog\\": "Modules/Catalog/app/",
            "Modules\\Catalog\\Database\\Factories\\": "Modules/Catalog/database/factories/",
            "Modules\\Catalog\\Database\\Seeders\\": "Modules/Catalog/database/seeders/",
            "Modules\\User\\": "Modules/User/app/",
            "Modules\\User\\Database\\Factories\\": "Modules/User/database/factories/",
            "Modules\\User\\Database\\Seeders\\": "Modules/User/database/seeders/",
            "Modules\\Review\\": "Modules/Review/app/",
            "Modules\\Review\\Database\\Factories\\": "Modules/Review/database/factories/",
            "Modules\\Review\\Database\\Seeders\\": "Modules/Review/database/seeders/",
            "Modules\\Order\\": "Modules/Order/app/",
            "Modules\\Order\\Database\\Factories\\": "Modules/Order/database/factories/",
            "Modules\\Order\\Database\\Seeders\\": "Modules/Order/database/seeders/",
            "Modules\\Payment\\": "Modules/Payment/app/",
            "Modules\\Payment\\Database\\Factories\\": "Modules/Payment/database/factories/",
            "Modules\\Payment\\Database\\Seeders\\": "Modules/Payment/database/seeders/",
            "Modules\\Coupon\\": "Modules/Coupon/app/",
            "Modules\\Coupon\\Database\\Factories\\": "Modules/Coupon/database/factories/",
            "Modules\\Coupon\\Database\\Seeders\\": "Modules/Coupon/database/seeders/",
            "Modules\\Offer\\": "Modules/Offer/app/",
            "Modules\\Offer\\Database\\Factories\\": "Modules/Offer/database/factories/",
            "Modules\\Offer\\Database\\Seeders\\": "Modules/Offer/database/seeders/",
            "Modules\\Favorite\\": "Modules/Favorite/app/",
            "Modules\\Favorite\\Database\\Factories\\": "Modules/Favorite/database/factories/",
            "Modules\\Favorite\\Database\\Seeders\\": "Modules/Favorite/database/seeders/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}